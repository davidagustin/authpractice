{
  "name": "authpractice-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter authpractice dev",
    "build": "pnpm --filter authpractice build",
    "start": "pnpm --filter authpractice start",
    "lint": "pnpm --filter authpractice lint",
    "release": "semantic-release",
    "docker:build": "docker build -t authpractice .",
    "docker:run": "docker run -d -p 3000:3000 --name authpractice_container authpractice"
  },
  "devDependencies": {
    "semantic-release": "^24.2.6",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3"
  }
} 